{
  "manifest_version": 3,
  "name": "My Chrome Extension",
  "version": "1.0.0",
  "background": {
    "service_worker": "/assets/background.js"
  },
  "content_scripts": [
    {
      "js": ["/assets/content.js"],
      "matches": ["https://www.google.com/*"]
    }
  ],
  "action": {
    "default_popup": "/popup/index.html"
  }
}
